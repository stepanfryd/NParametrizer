<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<AssemblyVersion>2.0.0.0</AssemblyVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Description>Simple parametrizing library to map arguments, appSettings and connectionStrings into class properties</Description>
		<Copyright>Copyright (c) Stepan Fryd 2018</Copyright>
		<PackageLicenseUrl>https://opensource.org/licenses/Apache-2.0</PackageLicenseUrl>
		<RepositoryUrl>https://github.com/stepanfryd/NParametrizer</RepositoryUrl>
		<RepositoryType>GIT</RepositoryType>
		<PackageProjectUrl>https://stepanfryd.github.io/NParametrizer/</PackageProjectUrl>
		<Authors>Stepan Fryd</Authors>
		<Company>Cloud17</Company>
		<FileVersion>2.0.0.0</FileVersion>
		<Version>2.0.0</Version>
	</PropertyGroup>
	<ItemGroup>
	  <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.5.0" />
	</ItemGroup>
</Project>